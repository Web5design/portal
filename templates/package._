<h2><%= p('title') %></h2>
<div id="main-content">
  <ul class="package-meta">
    <li><span class='label'><%= l('url') %>:</span> <%= p('url') %></li>
    <li><span class='label'><%= l('version') %>:</span> <%= p('version') %></li>
    <li><span class='label'><%= l('notes') %>:</span> <%= p('notes') %></li>
  </ul>

  <div class='resources'>
    <h3>Resources</h3>
    <ul><% _(resources).each(function(v) { %>
      <li><%= v.p('format') %> <%= v.p('description') %> <%= v.p('url') %></li>
    <% }); %></ul>
  </div>
</div>

<div id="supporting-content">
  <ul class="package-meta">
    <li><span class='label'><%= l('author') %>:</span> <%= p('author') %></li>
    <li><span class='label'><%= l('maintainer') %>:</span> <%= p('maintainer') %></li>
    <li><span class='label'><%= l('license') %>:</span> <%= p('license') %></li>
  </ul>
  <h3>Tags</h3>
  <ul class="package-tags">
    <li>Foo</li>
    <li>Bar</li>
  </ul>
  <h3>Groups</h3>
  <ul class="package-groups">
    <li>Baz</li>
  </ul>
<div>
